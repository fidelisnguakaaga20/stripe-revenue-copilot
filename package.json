{
  "name": "stripe-revenue-copilot",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "postinstall": "prisma generate",
    "cron:dunning": "node -e \"(async()=>{const s=process.env.CRON_SECRET;if(!s){console.error('Set CRON_SECRET in env');process.exit(1)}const port=process.env.PORT||'3000';const r=await fetch(`http://localhost:${port}/api/cron/dunning`,{method:'POST',headers:{'x-cron-key':s}});console.log(await r.text())})()\""
  },
  "dependencies": {
    "@prisma/client": "6.18.0",
    "jose": "^5.8.0",
    "next": "16.0.1",
    "nodemailer": "^6.9.14",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "stripe": "^14.25.0",
    "zod": "^3.23.8",
    "google-auth-library": "^9.14.2"
  },
  "devDependencies": {
    "@types/node": "^22.7.8",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18.3.11",
    "eslint": "^9.12.0",
    "eslint-config-next": "16.0.1",
    "prisma": "6.18.0",
    "typescript": "^5.6.3"
  }
}
